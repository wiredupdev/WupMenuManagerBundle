<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="wud_menu_manager" class="Wiredupdev\MenuManagerBundle\Menu\Manager" />

        <service id="wud_menu_manager.processor" class="Wiredupdev\MenuManagerBundle\Menu\Processor" />

        <service id="wud_menu_manager.twig_extension" class="Wiredupdev\MenuManagerBundle\Twig\MenuManagerExtension">
            <tag name="twig.extension"/>
            <argument type="service" id="wud_menu_manager" />
            <argument type="service" id="wud_menu_manager.processor" />
            <argument type="service" id="twig" />
        </service>
    </services>
</container>